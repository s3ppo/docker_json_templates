{
  "name": "immich",
  "repo": "ghcr.io/imagegenius/immich",
  "category": [ "media" ],
  "registry": "https://github.com/orgs/imagegenius/packages/container/package/immich",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "bash",
  "privileged": false,
  "extra_parameters": null,
  "post_parameters": null,
  "cpu_set": null,
  "web_ui_url": null,
  "icon": "https://raw.githubusercontent.com/imagegenius/templates/main/unraid/img/immich.png",
  "project": "https://immich.app/",
  "support": "https://github.com/imagegenius/docker-immich/issues/new/choose",
  "description": "Immich is a high performance self-hosted photo and video backup solution.",
  "paths": [
    {
      "name": "Appdata",
      "host": "/mnt/cache/appdata/immich/config",
      "container": "/config",
      "mode": "rw",
      "description": "Contains machine learning models (~1.5GB with default models)",
      "required": true
    },
    {
      "name": "Path: /photos",
      "host": "/mnt/cache/appdata/immich/photos",
      "container": "/photos",
      "mode": "rw",
      "description": "Contains all the photos uploaded to Immich",
      "required": true
    },
    {
      "name": "Path: /libraries",
      "host": "/mnt/cache/appdata/immich/libraries",
      "container": "/libraries",
      "mode": "rw",
      "description": "External libraries to track assets stored outside of Immich",
      "required": false
    }
  ],
  "ports": [
    {
      "name": "WebUI",
      "host": "8080",
      "container": "8080",
      "protocol": "tcp",
      "description": "WebUI Port",
      "required": true,
      "mask": false
    }
  ],
  "variables": [
    {
      "name": "DB_HOSTNAME",
      "key": "DB_HOSTNAME",
      "value": "192.168.1.x",
      "description": "PostgreSQL Host",
      "required": true,
      "mask": false
    },
    {
      "name": "DB_USERNAME",
      "key": "DB_USERNAME",
      "value": "postgres",
      "description": "PostgreSQL Username",
      "required": true,
      "mask": false
    },
    {
      "name": "DB_PASSWORD",
      "key": "DB_PASSWORD",
      "value": "postgres",
      "description": "PostgreSQL Password",
      "required": true,
      "mask": true
    },
    {
      "name": "DB_DATABASE_NAME",
      "key": "DB_DATABASE_NAME",
      "value": "immich",
      "description": "PostgreSQL Database Name",
      "required": true,
      "mask": false
    },
    {
      "name": "REDIS_HOSTNAME",
      "key": "REDIS_HOSTNAME",
      "value": "192.168.1.x",
      "description": "Valkey/Redis Hostname",
      "required": true,
      "mask": false
    },
    {
      "name": "DB_PORT",
      "key": "DB_PORT",
      "value": "5432",
      "description": "PostgreSQL Port",
      "required": false,
      "mask": false
    },
    {
      "name": "REDIS_PORT",
      "key": "REDIS_PORT",
      "value": "6379",
      "description": "Valkey/Redis Port",
      "required": false,
      "mask": false
    },
    {
      "name": "REDIS_PASSWORD",
      "key": "REDIS_PASSWORD",
      "value": null,
      "description": "Valkey/Redis password",
      "required": false,
      "mask": true
    },
    {
      "name": "SERVER_HOST",
      "key": "SERVER_HOST",
      "value": "0.0.0.0",
      "description": "Immich server host",
      "required": false,
      "mask": false
    },
    {
      "name": "SERVER_PORT",
      "key": "SERVER_PORT",
      "value": "8080",
      "description": "Immich server port",
      "required": false,
      "mask": false
    },
    {
      "name": "MACHINE_LEARNING_HOST",
      "key": "MACHINE_LEARNING_HOST",
      "value": "0.0.0.0",
      "description": "Immich machine-learning host",
      "required": false,
      "mask": false
    },
    {
      "name": "MACHINE_LEARNING_PORT",
      "key": "MACHINE_LEARNING_PORT",
      "value": "3003",
      "description": "Immich machine-learning port",
      "required": false,
      "mask": false
    },
    {
      "name": "MACHINE_LEARNING_WORKERS",
      "key": "MACHINE_LEARNING_WORKERS",
      "value": "1",
      "description": "Machine learning workers",
      "required": false,
      "mask": false
    },
    {
      "name": "MACHINE_LEARNING_WORKER_TIMEOUT",
      "key": "MACHINE_LEARNING_WORKER_TIMEOUT",
      "value": "120",
      "description": "Machine learning worker timeout",
      "required": false,
      "mask": false
    },
    {
      "name": "PUID",
      "key": "PUID",
      "value": "500",
      "description": "UID for permissions. Do not change unless you know what you're doing.",
      "required": true,
      "mask": false
    },
    {
      "name": "PGID",
      "key": "PGID",
      "value": "500",
      "description": "GID for permissions. Do not change unless you know what you're doing.",
      "required": true,
      "mask": false
    },
    {
      "name": "UMASK",
      "key": "UMASK",
      "value": "022",
      "description": "UMASK for newly created files. Do not change unless you know what you're doing.",
      "required": false,
      "mask": false
    }
  ],
  "devices": [],
  "labels": []
}
