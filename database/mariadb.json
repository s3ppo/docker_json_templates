{
  "name": "mariadb",
  "repo": "lscr.io/linuxserver/mariadb",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "bash",
  "privileged": false,
  "extra_parameters": null,
  "web_ui_url": null,
  "icon": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/mariadb-logo.png",
  "paths": [
    {
      "name": "Appdata",
      "mode": "rw",
      "host": "/mnt/cache/appdata/mariadb",
      "container": "/config"
    }
  ],
  "ports": [
    {
      "name": "WebUI",
      "protocol": "tcp",
      "host": "3306",
      "container": "3306"
    }
  ],
  "variables": [
    {
      "name": "MYSQL_ROOT_PASSWORD",
      "key": "MYSQL_ROOT_PASSWORD",
      "value": "mariadb",
      "mask": true
    },
    {
      "name": "MYSQL_DATABASE",
      "key": "MYSQL_DATABASE",
      "value": "mariadb",
      "mask": true
    },
    {
      "name": "MYSQL_USER",
      "key": "MYSQL_USER",
      "value": "mariadb",
      "mask": true
    },
    {
      "name": "MYSQL_PASSWORD",
      "key": "MYSQL_PASSWORD",
      "value": "mariadb",
      "mask": true
    },
    {
      "name": "REMOTE_SQL",
      "key": "REMOTE_SQL",
      "value": "http://URL1/your.sql,https://URL2/your.sql",
      "mask": false
    },
    {
      "name": "PUID",
      "key": "PUID",
      "value": "500",
      "mask": false
    },
    {
      "name": "PGID",
      "key": "PGID",
      "value": "500",
      "mask": false
    },
    {
      "name": "UMASK",
      "key": "UMASK",
      "value": "022",
      "mask": false
    }
  ],
  "devices": []
}