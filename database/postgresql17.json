{
  "name": "postgresql17",
  "repo": "postgres:17",
  "network": "bridge",
  "custom_ip": "",
  "default_shell": "",
  "privileged": false,
  "extra_parameters": "",
  "web_ui_url": "",
  "icon": "https://raw.githubusercontent.com/sgraaf/Unraid-Docker-Templates/main/postgresql17/icon.png",
  "paths": [
    {
      "name": "Path: /var/lib/postgresql/data",
      "mode": "rw",
      "host": "/mnt/cache/appdata/postgresql/data",
      "container": "/var/lib/postgresql/data"
    }
  ],
  "ports": [
    {
      "name": "Port: PostgreSQL access port",
      "protocol": "tcp",
      "host": "5432",
      "container": "5432"
    }
  ],
  "variables": [
    {
      "name": "Variable: POSTGRES_PASSWORD",
      "key": "POSTGRES_PASSWORD",
      "value": "postgres",
      "mask": false
    },
    {
      "name": "Variable: POSTGRES_USER",
      "key": "POSTGRES_USER",
      "value": "postgres",
      "mask": false
    },
    {
      "name": "Variable: POSTGRES_DB",
      "key": "POSTGRES_DB",
      "value": "postgres",
      "mask": false
    }
  ],
  "devices": [],
  "gpus": []
}